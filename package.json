{
  "name": "anamnese-mobile",
  "version": "1.0.0",
  "description": "DSGVO-konforme medizinische Anamnese App",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "windows": "react-native run-windows",
    "windows:run": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-dev-run.ps1",
    "windows:run:log": "cmd.exe /d /c \"powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-dev-run.ps1 > buildLogs\\windows-dev-run_latest.out.log 2> buildLogs\\windows-dev-run_latest.err.log\"",
    "windows:run:skipbuild": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-dev-run.ps1 -SkipBuild",
    "windows:run:skipbuild:log": "cmd.exe /d /c \"powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-dev-run.ps1 -SkipBuild > buildLogs\\windows-dev-run_skipbuild_latest.out.log 2> buildLogs\\windows-dev-run_skipbuild_latest.err.log\"",
    "windows:launch:log": "cmd.exe /d /c \"powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-launch.ps1 > buildLogs\\windows-launch_latest.out.log 2> buildLogs\\windows-launch_latest.err.log\"",
    "windows:cleanrun": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-cleanrun.ps1",
    "start": "react-native start",
    "test": "jest",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "triage:build": "node scripts/triage-run.js --name build -- npm run type-check",
    "triage:test": "node scripts/triage-run.js --name test -- npm test",
    "triage:build-and-test": "node scripts/triage-pipeline.js",
    "postinstall": "patch-package",
    "generate:template": "node scripts/generate-questionnaire-template.js",
    "windows:init": "node node_modules\\react-native-windows-init\\bin.js --overwrite",
    "windows:deps": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-deps.ps1",
    "windows:setup": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-setup.ps1"
    ,
    "windows:ready": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-ready-to-test.ps1",
    "windows:ready:skipbuild": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-ready-to-test.ps1 -SkipBuild",
    "windows:ready:skiptests": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-ready-to-test.ps1 -SkipTests",
    "windows:ready:skipmetro": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-ready-to-test.ps1 -SkipMetroCheck",
    "windows:ready:skipbuild:skiptests": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File scripts\\windows-ready-to-test.ps1 -SkipBuild -SkipTests"
  },
  "dependencies": {
    "@noble/ciphers": "0.5.1",
    "@noble/hashes": "1.3.3",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-clipboard/clipboard": "1.16.3",
    "@react-native-voice/voice": "^3.1.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@types/crypto-js": "^4.2.1",
    "buffer": "6.0.3",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.0.6",
    "i18next": "^23.7.11",
    "immer": "^10.0.3",
    "react": "18.2.0",
    "react-i18next": "^14.0.0",
    "react-native": "^0.73.11",
    "react-native-date-picker": "^4.3.3",
    "react-native-document-picker": "^9.1.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-get-random-values": "^1.10.0",
    "react-native-keychain": "^8.1.2",
    "react-native-quick-crypto": "^0.7.0",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-share": "^10.0.2",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-tesseract-ocr": "^2.0.0",
    "react-native-tts": "^4.1.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-windows": "0.73.22",
    "react-native-worklets": "^0.7.1",
    "shared": "file:./shared",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/runtime": "^7.23.7",
    "@react-native/babel-preset": "^0.73.19",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.3",
    "@react-native/typescript-config": "^0.73.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.47",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.14.8",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "metro-config": "^0.80.12",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.1",
    "react-native-windows-init": "0.81.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "private": true
}
